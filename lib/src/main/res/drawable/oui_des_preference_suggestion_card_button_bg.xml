<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?colorControlHighlight">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
            <corners android:radius="@dimen/oui_des_suggestionpref_round_button_bg_radius" />
            <padding
                android:bottom="@dimen/oui_des_suggestionpref_round_button_padding_top_bottom"
                android:left="@dimen/oui_des_suggestionpref_round_button_padding_start_end"
                android:right="@dimen/oui_des_suggestionpref_round_button_padding_start_end"
                android:top="@dimen/oui_des_suggestionpref_round_button_padding_top_bottom" />
        </shape>
    </item>
    <item android:id="@+id/rec_shape">
        <shape
            android:shape="rectangle">
            <solid android:color="?colorPrimary" />
            <corners android:radius="@dimen/oui_des_suggestionpref_round_button_bg_radius" />
            <padding
                android:bottom="@dimen/oui_des_suggestionpref_round_button_padding_top_bottom"
                android:left="@dimen/oui_des_suggestionpref_round_button_padding_start_end"
                android:right="@dimen/oui_des_suggestionpref_round_button_padding_start_end"
                android:top="@dimen/oui_des_suggestionpref_round_button_padding_top_bottom" />
        </shape>
    </item>
</ripple>